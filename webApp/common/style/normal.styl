// facebook和google登陆提示
.login-panel
  display none
  position fixed
  left 50%
  top 200px
  margin-left -320px
  width 640px
  height 334px
  padding 20px
  text-align center
  border-radius 4px
  background-color rgba(255, 255, 255, .9)
  box-shadow 0 0 7px 0 #efefef
  z-index 999
  .close
    font-size 2rem
    color #4F6D95
    opacity 1
  p
    margin 35px 0
    color #33475B
    font-size 3rem
  button
    display block
    margin 0 auto 20px auto
    width 320px
    font-size 1.8rem
    text-align left
    i
      display inline-block
      width 56px
      font-size 3rem
      border-right 1px solid #fff
      text-align center
    .glyphicon-gg
      font-size 2rem
    span
      margin-left 43px
      font-weight normal
      vertical-align top
.login-bar
  display none
  position fixed
  bottom 0
  left 0
  width 100%
  height 120px
  line-height 120px
  // padding-left 139px
  background-color rgba(255, 255, 255, .9)
  box-shadow 0 0 7px 0 #efefef
  z-index 99
.login-bar-inner
  margin 0 auto
  float none
  // width 1100px
  button
    margin 34px 0
  .btn-face
    margin-left 33px
    i
      font-size 2rem
  .btn-gg
    margin-left 40px
    i
      font-size 2.4rem
  &>span
    color #4F6D95
    font-size 2.4rem
    font-weight bold

.btn-face, .btn-gg
  padding 0
  width 180px
  height 56px
  line-height 56px
  color #fff
  background-color #3B5791
  border-radius 4px
  border 0
  vertical-align middle
  &:hover
    background-color darken(#3B5791, 20%)
  i
    color #fff
.btn-gg
  background-color #F24033
  &:hover
    background-color darken(#F24033, 20%)
