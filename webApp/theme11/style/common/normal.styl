input[type=number]::-webkit-inner-spin-button,
input[type=number]::-webkit-outer-spin-button
  -webkit-appearance none
  margin 0
svg.css-shape
  display block !important
  html.safari &
    display none !important

@font-face {
  font-family: 'buyer-theme_11';
  src: url('/assets/theme11/font/buyer-theme_11.eot?19322747');
  src: url('/assets/theme11/font/buyer-theme_11.eot?19322747#iefix') format('embedded-opentype'),
       url('/assets/theme11/font/buyer-theme_11.woff?19322747') format('woff'),
       url('/assets/theme11/font/buyer-theme_11.ttf?19322747') format('truetype'),
       url('/assets/theme11/font/buyer-theme_11.svg?19322747#buyer-theme_11') format('svg');
  font-weight: normal;
  font-style: normal;
}

.theme-icon
  font-family: "buyer-theme_11";
  font-style: normal;
  font-weight: normal;
  speak: none;
  display: inline-block;
  text-decoration: inherit;
  width: 1em;
  margin-right: .2em;
  text-align: center;
  // For safety - reset parent styles, that can break glyph codes
  font-variant: normal;
  text-transform: none;
  // fix buttons height, for twitter bootstrap
  // line-height: 1em;
  // Animation center compensation - margins should be symmetric
  // remove if not needed
  // margin-left: .2em;
  // You can be more comfortable with increased icons size
  // font-size 120%
