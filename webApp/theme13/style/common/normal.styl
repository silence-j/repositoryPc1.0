.btn
  display inline-block
  border-radius 20px
  .glyphicon
    margin-top -5px
.btn-default
  height 40px
  background-color $white
  border 2px solid $grayO
  font-size 14px
  .glyphicon
    color $mainColor
  &:hover, &:focus
    text-decoration none
    border-color $mainColor
    background-color $mainColor
    color $white
    .glyphicon
      color $white

.btn-blue, .btn-send
  width 130px
  height 40px
  line-height 36px
  text-align center
  color $white
  background $secondaryColor
  border 2px solid $secondaryColor
  word-break break-all
  text-decoration none
  &.btn-flat
    color $white
    background $mainColor
    border-color $mainColor
.btn-blue:hover, .btn-blue:focus, .btn-blue:active,
.btn-send:hover, .btn-send:focus, .btn-send:active
  color $white
  background $mainColor
  border-color $mainColor
  text-decoration none

.btn-send
  padding 0
  margin 0 auto
  display block
  height 40px
  background-color $secondaryColor
  border 2px solid $secondaryColor
  color $white
  line-height 36px
  border-radius 20px
.btn-send[disabled]
  background-color $whiteOO
  border-color $grayO
  color $blackOOO

.btn-blue-trans
  padding 10px 30px
  background $secondaryColor
  border 2px solid $secondaryColor
  color $white
  border-radius 20px
.btn-blue-trans:hover, .btn-blue-trans:focus, .btn-blue-trans:active
  color $white
  background $mainColor
  border-color $mainColor

.btn-black-trans
  // padding 12px 30px
  display inline-block
  text-align center
  paddding 0
  height 50px
  line-height 46px
  background $white
  border 1px solid
  color $blackTrue
  font-size 1.6rem
  font-weight bold
  cursor pointer
.btn-black-trans:hover, .btn-black-trans:focus, .btn-black-trans:active
  background $white
  border 1px solid $mainColor
  color $mainColor

.caret
  &.caret-right
    border-left 7px dashed
    border-left 7px solid \9
    border-top 7px solid transparent
    border-bottom 7px solid transparent
    border-right none
  &.caret-up
    border-bottom 4px dashed
    border-bottom 4px solid \9
    border-left 4px solid transparent
    border-right 4px solid transparent
    border-top none
  &.caret14
    border-width 7px !important

.ui-select, .ui-input
  padding 6px 12px
  width 100%
  height 40px
  -webkit-appearance none
  background-image url("data:image/png;base64,")
  background-repeat no-repeat
  background-position 98% center
  border-radius 0
  border 0
  background-color $selectBgColor
  color $selectColor
  border 1px solid $selectBorderColor
.ui-select:hover
  border-color $mainColor
  background-image url("data:image/png;base64,")
.ui-input
  background-image none

.select3-hidden
  display none
  visibility hidden
  padding-right 10px
.select3
  cursor pointer
  display inline-block
  position relative
  width 50%
  height 40px
  vertical-align middle
  font-size 16px
  color $selectItemColor
.select3-styled
  position absolute
  top 0
  right 0
  bottom 0
  left 0
  background-color $selectBgColor
  color $selectColor
  padding 8px 35px 8px 15px
  transition all 0.2s ease-in
  border 1px solid $selectBorderColor
  border-radius 4px
  text-align center
  .chevron-down, .chevron-up
    position absolute
    top 10px
    right 10px
    display inline-block
    font-size 12px
    color $selectColor
  .chevron-up
    display none
  &:after
    display none
    content ""
    width 0
    height 0
    border 7px solid transparent
    border-color $selectItemColor transparent transparent transparent
    position absolute
    top 16px
    right 10px
  &:hover
    // background-color darken($selectBgColor, 2)
    background-color $selectBgColor
    border-color $selectHoverBorderColor
  &:active, &.active
    // background-color darken($selectBgColor, 5)
    background-color $selectBgColor
    .chevron-down
      display none
    .chevron-up
      display inline-block
    &:after
      display none
      top 9px
      border-color transparent transparent $selectItemColor transparent
.select3-options
  display none
  position absolute
  top 100%
  right 0
  left 0
  z-index 999
  margin 0
  padding 0
  list-style none
  background-color darken($selectBgColor, 5)
  box-shadow 0 0 6px 0 rgba(0,0,0,0.20)
  li
    margin 0
    padding 12px 0
    text-indent 15px
    // border-top 1px solid darken($selectBgColor, 10)
    transition all 0.15s ease-in
    background-color $selectItemBgColor
    color $selectItemColor
    &:hover
      background-color $selectItemHoverBgColor
      color $selectItemHoverColor
    &[rel="hide"]
      display none

.modal
  text-align: center;
  &:before
    display inline-block
    vertical-align middle
    content " "
    height 100%
.modal-dialog
  display inline-block
  text-align left
  vertical-align middle

.module-title
  background-color transparent
  position relative
  font-weight bold
  font-size 24px
  margin-bottom 30px
  // &::first-letter
  //   color $mainColor
  .caret
    display none
    color $mainColor
    margin-left 10px
  .line
    display inline-block
    position absolute
    background-color $mainColor
    height 5px
    width 40px
    bottom -5px
    left 0

.pagination>li
  display inline-block
  margin 0 5px
.pagination>li>a, .pagination>li>span
  padding 0
  width 28px
  height 28px
  line-height 28px
  text-align center
  color #666
  border-radius 4px
.pagination>li:first-child>a, .pagination>li:first-child>span,
.pagination>li:last-child>a, .pagination>li:last-child>span
  border-radius 4px
  span
    font-size 14px
.pagination>li>a:hover, .pagination>li>span:hover, .pagination>li>a:focus, .pagination>li>span:focus
  border 1px solid $mainColor
  color $white
  background $mainColor
  cursor pointer
.pagination>.active>a, .pagination>.active>span
   background-color $secondaryColor
   border-color $secondaryColor

