    .modal-content
       background none
       box-shadow none
       border none
li
  list-style none
/*内嵌编辑  蒙层*/
.iframe-layer{
  width: 100%;
  height: 100%;
  z-index: 500;
  background: #fff;
  position: fixed;
  left: 0;
  top: 0;
  opacity: 0;
}
/*404页面*/
.error_page
  background #fff
  background #fff
  height 500px
  width 50%
  position absolute
  top 50%
  margin-top -250px
  left 25%
  min-width: 580px;
  .center_model
    width 100%
    height 100%
    margin 0 auto
    display table
    text-align center
    .conent
      display table-cell
      vertical-align middle
      .left
        width 50%
        display inline-block
        text-align right
        padding-right 27px
        vertical-align top
        margin-top 60px
        .light
          font-size 180px
      .right
        width 50%
        display inline-block
        text-align left
        padding-left 27px
        .font404
          font-size 100px
          color #5488F9
        .desc
          margin 0 0
          font-szie 14px
          color #333
          font-weight 600
        .error
          margin-top 12px
          font-size 12px
          color #b0c1d4
          .label_desc
            display inline-block
            vertical-align top
          .error_prompt
            display inline-block
            margin 0 0
            li
              list-style-type disc
        .back_btn
          margin-top 20px
          font-weight 600
          i
            margin-left 20px
            cursor pointer
// 模板上的下拉选择内容太长
.select3-styled
