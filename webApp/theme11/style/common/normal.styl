.btn
  display inline-block
  .glyphicon
    margin-top -5px
.btn-default
  height 40px
  background-color $white
  //border 2px solid $grayO
  color $black
  font-size 14px
  .glyphicon
    color $mainColor
  &:hover, &:focus
    text-decoration none
    border-color $mainColor
    background-color $white
    color $black
    .glyphicon
      color $mainColor

.btn-blue, .btn-send
  width 130px
  height 40px
  line-height 36px
  text-align center
  color $mainColor
  background $white
  border 2px solid $mainColor
  word-break break-all
  text-decoration none
  &.btn-flat
    color $white
    background $mainColor
.btn-blue:hover, .btn-send:hover
  color $white
  background $mainColor
  text-decoration none
.btn-blue-trans
  padding 10px 30px
  background $white
  border 2px solid $mainColor
  color $mainColor
.btn-blue-trans:hover
  background $mainColor
  color $white

.btn-black-trans
  // padding 12px 30px
  display inline-block
  text-align center
  paddding 0
  height 50px
  line-height 46px
  background $white
  border 2px solid #000
  color #000
  font-size 1.6rem
  font-weight bold
  cursor pointer
.btn-black-trans:hover
  background $white
  border 2px solid $mainColor
  color $mainColor

.btn-send
  padding 0
  margin 0 auto
  display block
  height 40px
  background-color $mainColor
  color $forgroundColor
  line-height 36px
.btn-send[disabled]
  background-color $whiteOO
  color $blackOOO
  border 2px solid $grayO

.caret
  &.caret-right
    border-left 7px dashed
    border-left 7px solid \9
    border-top 7px solid transparent
    border-bottom 7px solid transparent
    border-right none
  &.caret-up
    border-bottom 7px dashed
    border-bottom 7px solid \9
    border-left 7px solid transparent
    border-right 7px solid transparent
    border-top none
  &.caret14
    border-top 7px dashed
    border-top 7px solid \9
    border-right 7px solid transparent
    border-left 7px solid transparent

.ui-select, .ui-input
  padding 6px 12px
  width 100%
  height 40px
  -webkit-appearance none
  background-image url("data:image/png;base64,")
  background-repeat no-repeat
  background-position 98% center
  border-radius 0
  border 0
  background-color $grayOOO
  color $blackO
  border 1px solid $grayOOO
.ui-select:focus
  outline none
.ui-select:hover
  border-color $mainColor
  background-image url("data:image/png;base64,")
.ui-input
  background-image none

.modal
  text-align: center;
  &:before
    display inline-block
    vertical-align middle
    content " "
    height 100%
.modal-dialog
  display inline-block
  text-align left
  vertical-align middle
  .modal-content
     background none
.module-title
  background-color transparent
  position relative
  font-weight bold
  font-size 24px
  margin-bottom 30px
  &::first-letter
    color $mainColor
  .caret
    color $mainColor
    margin-left 10px
  .line
    position absolute
    background-color $black
    height 3px
    width 100%
    bottom -10px
    left 0

.pagination>li
  display inline-block
  margin 0 5px
.pagination>li>a, .pagination>li>span
  padding 0
  width 28px
  height 28px
  line-height 28px
  text-align center
  color #666
.pagination>li:first-child>a, .pagination>li:first-child>span,
.pagination>li:last-child>a, .pagination>li:last-child>span
  border-radius 0
  span
    font-size 14px
.pagination>li>a:hover, .pagination>li>span:hover, .pagination>li>a:focus, .pagination>li>span:focus
  border 1px solid $mainColor
  color $mainColor
  background $white
  cursor pointer
.pagination>.active>a, .pagination>.active>span
  background-color $mainColor
  border-color $mainColor
  &:hover
    background-color $mainColor
    border-color $mainColor

/* 表单 focus加样式.form-group-focus;blur 去掉focus样式 根据是否输入 添加form-group-min样式 验证样式error*/
.valid-form
  .form-group
    position relative
    label
      // position absolute
      // top 16px
      width 110px
      font-size 14px
      color $black
      font-weight normal
      margin-bottom 15px
  .form-group.form-group-focus
    border-color $mainColor
    label
      // top 3px
      // font-size 12px
      // color $mainColor
  .form-group-min
    label
      // top 3px
      // font-size 12px

.validate-tip
  display none
  position absolute
  right 0
  top 0
.error
  border-color $errorColor !important
  label
    span
      // color $errorColor
      color $black
  .validate-tip
    display block
    // min-height 18px
    // padding 10px
    // background-color #FFEEE6
    color $errorColor
    i[class^="glyphicon"]
      margin-right 6px
.empty-input
  // padding 10px 0 0 10px
  padding 0 10px
  width 100%
  height 100%
  outline none
  // min-height 50px
  min-height 40px
  line-height 40px
  // border 0
  border 1px solid $grayO

// 提示框
.alert
  position fixed
  z-index 10000
  top 100px
  left 50%
  margin-left -242px
  width 585px
  padding 4px 0 4px 15px
  color #666
  word-break break-all
  border-radius 4px
  i
    margin-right 10px
    font-size 14px
.alert-success
  border 1px solid #e3f4db
  background-color #f3faf0
.alert-info
  border 1px solid #cfe8fa
  background-color #e7f3fc
.alert-warning
  border 1px solid #fec
  background-color #FFFAEA

.learn-more-cntr
  position relative
  display none
.learn-more
  // position absolute
  // width 50%
  // top 45%
  // left 25%
  // box-shadow:0 0 10px rgba(53, 154, 246, .8)
  display block
  margin-top 10px
  width 100%
  color $forgroundColor !important
  cursor pointer

.cd-top
  display inline-block
  position fixed
  bottom 66px
  right 10px
  height 40px
  width 40px
  overflow hidden
  white-space nowrap
  text-align center
  font-size 22px
  text-decoration none
  background $mainColor
  border 2px solid $mainColor
  visibility hidden
  opacity 0
  box-shadow 0 0 10px rgba(0, 0, 0, 0.05)
  -webkit-transition opacity .3s 0s, visibility 0s .3s
  -moz-transition opacity .3s 0s, visibility 0s .3s
  transition opacity .3s 0s, visibility 0s .3s
  z-index 899
  &:hover
    text-decoration none
  &.cd-is-visible,
  &.cd-fade-out
    -webkit-transition opacity .3s 0s, visibility 0s 0s
    -moz-transition opacity .3s 0s, visibility 0s 0s
    transition opacity .3s 0s, visibility 0s 0s
  &.cd-is-visible
    visibility visible
    opacity 1
  &.cd-fade-out
    opacity .5
  .theme-icon
    width 36px
    line-height 36px
    font-size 20px
    color $white

@media (min-width: 769px) and (max-width: 809px)
  .cd-top
    right 0
    left auto
    margin-left 0
@media (min-width: 810px)
  .cd-top
    right auto
    left 50%
    margin-left unit(750/2, 'px')
@media (min-width: 993px) and (max-width: 1033px)
  .cd-top
    right 0
    left auto
    margin-left 0
@media (min-width: 1034px)
  .cd-top
    right auto
    left 50%
    margin-left unit(970/2, 'px')
@media (min-width: 1201px) and (max-width: 1243px)
  .cd-top
    right 0
    left auto
    margin-left 0
@media (min-width: 1244px)
  .cd-top
    right auto
    left 50%
    margin-left unit(1200/2, 'px')

@media screen and (max-width: 767px)
  .validate-tip
    position static
    margin-top 10px

// responsive redefine
@media (min-width: 1200px)
  .container
    width 1200px
